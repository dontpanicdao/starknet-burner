{
  "name": "@starknet/burner",
  "private": false,
  "version": "0.4.0-alpha1",
  "description": "Starknet Burner",
  "type": "module",
  "module": "./src/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/dontpanicdao/starknet-burner",
  "keywords": [
    "starknet",
    "burner",
    "wallet"
  ],
  "scripts": {
    "dev": "vite",
    "dev:build": "tsc && vite build --mode development",
    "doc": "typedoc --plugin typedoc-plugin-markdown --options ./typedoc.json",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prebuild": "node ./scripts/prebuild.js",
    "postbuild": "node ./scripts/postbuild.js",
    "test:e2e": "playwright test",
    "test:jest": "jest"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@types/bn.js": "^5.1.1",
    "@types/elliptic": "^6.4.14",
    "bn.js": "^5.2.1",
    "jest": "^28.1.3",
    "starknet3x": "npm:starknet@3.18.2",
    "starknet4x": "npm:starknet@4.4.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.19",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "vite": "^3.2.0"
  },
  "contributors": [
    {
      "name": "gregoryguillou"
    },
    {
      "name": "0xjilan"
    }
  ],
  "license": "MIT"
}
